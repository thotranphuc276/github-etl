# GitHub ETL Pipeline Configuration

# GitHub API token (required for higher rate limits)
GITHUB_TOKEN=your_token_here

# Repository to analyze (format: owner/repo_name)
GITHUB_REPO=apache/airflow

# Number of months to look back for commits
MONTHS=6

# Path to SQLite database file
DB_PATH=github_commits.db

# Whether to run analysis after ETL
RUN_ANALYSIS=true